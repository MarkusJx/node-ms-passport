        "Windows Hello",
        "Authentication",
        "Microsoft Passport",
        "Credential Manager",
        "Password Encryption"
    ],
    "author": "MarkusJx",
    "license": "MIT",
    "devDependencies": {
        "@types/node": "^16.10.2",
        "mocha": "^9.1.2"
    },
    "main": "index.js",
    "os": [
        "win32"
    ],
    "cpu": [
        "x64",
        "ia32"
    ],
    "dependencies": {
        "@markusjx/n-api-tools": "^1.1.0",
        "cmake-js": "^6.2.1",
        "node-addon-api": "^4.2.0"
    }

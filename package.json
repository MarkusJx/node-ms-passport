{
  "name": "node-ms-passport",
  "version": "1.0.0",
  "description": "Microsoft passport with windows hello and credential manager for node js",
  "repository": "https://github.com/MarkusJx/node-ms-passport",
  "scripts": {
    "build": "cmake-js compile",
    "postbuild": "node install.js --post_build",
    "install": "npm run-script build",
    "postinstall": "node install.js",
    "pretest": "npm run-script build",
    "test": "mocha",
    "clean": "node install.js --clean"
  },
  "keywords": [
    "Electron",
    "Windows Hello",
    "Authentication",
    "Microsoft Passport",
    "Credential Manager",
    "Password Encryption"
  ],
  "author": "MarkusJx",
  "license": "MIT",
  "devDependencies": {
    "mocha": "^8.2.1"
  },
  "main": "index.js",
  "os": [
    "win32"
  ],
  "cpu": [
    "x64",
    "ia32"
  ],
  "dependencies": {
    "@markusjx/n-api-tools": "^1.0.3",
    "cmake-js": "^6.1.0",
    "node-addon-api": "^3.1.0"
  }
}
